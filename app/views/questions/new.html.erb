<h1>Create A New Question</h1>
<% if @question.errors.any? %>
<% @question.errors.full_messages.each do |error| %>
<%= error %>
<div>&nbsp;</div>
<% end %>
<% end %>
<%= form_for @question do |f| %>
  <div>
    <%= f.label :question_name %>
    <%= f.text_field :question_name %>
  </div>
  <div>
    <%= f.label :description %>
    <%= f.text_area :description %>
  </div>
  <div>
    <%= f.submit "Submit" %>
  </div>

<% end %>
